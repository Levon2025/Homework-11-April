#include <stdio.h>

int cmp_str(const char *str1,const char *str2){
	while(*str1 && str2){
		if(*str1 != *str2){
			return (*str1 > *str2) ? 1: -1;
}
		str1++;
		str2++;
}
	if(*str1 == *str2){
		return 0;
}
	return (*str1 > *str2) ? 1 : -1;
}
int main(){
	const char *s1 = "Iphone";
	const char *s2 = "Samsung";
	int res = cmp_str(s1,s2);
	if(res == 0){
		printf("0");
	} else if(res < 0){
		printf("-1");
	}else{
	printf(	"1");
}

	return 0;
}
